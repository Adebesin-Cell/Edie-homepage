{
  "name": "edie-homepage",
  "version": "1.0.0",
  "description": "This is a homepage for Edie company",
  "main": "index.js",
  "scripts": {
    "watch": "node-sass -w scss/main.scss css/main.css",
    "server": "live-server",
    "start": "npm-run-all --parallel watch server",
    "compile": "node-sass scss/main.scss css/style.comp.css",
    "concat": "concat -o css/style.concat.css css/material-icons.css css/style.comp.css",
    "compress": "node-sass css/style.concat.css css/main.css --output-style compressed",
    "build": "npm-run-all compile concat compress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Adebesin-Cell/Edie-homepage.git"
  },
  "author": "Tolu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Adebesin-Cell/Edie-homepage/issues"
  },
  "homepage": "https://github.com/Adebesin-Cell/Edie-homepage#readme",
  "devDependencies": {
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "yarn-run-all": "^3.1.1"
  }
}
